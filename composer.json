{
    "name":"lionel/coin-slider",
    "description":"Integrate the slideshow Coin Slider into your website easily.",
    "keywords":["gallery", "Coin Slider", "slideshow"],
    "type":"contao-module",
    "license":"MIT",
    "authors":[
        {
            "name":"Lionel Maccaud",
            "email":"work@lionelmaccaud.ch",
            "homepage":"http://lionelmaccaud.ch",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"work@lionelmaccaud.ch",
        "issues":"https://github.com/lionel-m/coin-slider/issues?page=1&state=open",
        "source":"https://github.com/lionel-m/coin-slider"
    },
    "require":{
        "php":">=5.4.0",
        "contao/core-bundle": "4.*"
    },
    "replace": {
        "contao-legacy/coin-slider": "self.version"
    },
    "autoload": {
        "psr-4": {
            "LionelM\\CoinSliderBundle\\": "src/"
        }
    },
    "extra":{
        "contao":{
            "runonce":[
                "src/runonce/runonce.php"
            ]
        }
    }
}
